@font-face
  font-family 'icomoon'
  src url('../fonts/icomoon.eot?voppiv')
  src url('../fonts/icomoon.eot?voppiv#iefix') format('embedded-opentype'),
        url('../fonts/icomoon.ttf?voppiv') format('truetype'),
        url('../fonts/icomoon.woff?voppiv') format('woff'),
        url('../fonts/icomoon.svg?voppiv#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-inicio
