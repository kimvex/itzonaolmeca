.nav
  padding 0
  margin 0
  position absolute
  z-index 1001
  width 100%
  background white
  border-top .1em solid borde
  border-bottom .1em solid borde

.nav > li {
  float:left;
  width 20%
  text-align center
}

.nav li a {
  text-decoration:none;
  color: #666;
  padding:10px 12px;
  display:block;
  background: white;
}

.nav li a:hover {
  background-color:#666;
  color: white;
}

.nav li ul {
  display:none;
  position:absolute;
  min-width:170px;
  text-align left
}

.nav li:hover > ul {
  display:block;
}

.nav li ul li {
  position:relative;
}

.nav li ul li ul {
  right:-140px;
  top:0px;
}


